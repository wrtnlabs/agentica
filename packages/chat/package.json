    "@samchon/shopping-api": "^0.16.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-json-editor-ajrm": "^2.5.6",
    "@types/uuid": "^10.0.0",
