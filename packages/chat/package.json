{
  "name": "@agentica/chat",
  "type": "module",
  "version": "0.19.1",
  "description": "Frontend Application of Agentica",
  "author": "Wrtn Technologies",
  "license": "MIT",
  "homepage": "https://wrtnlabs.io/agentica",
  "repository": {
    "type": "git",
    "url": "https://github.com/wrtnlabs/agentica"
  },
  "bugs": {
    "url": "https://github.com/wrtnlabs/agentica/issues"
  },
  "keywords": [
    "openapi",
    "swagger",
    "generator",
    "typescript",
    "editor",
    "sdk",
    "nestjs",
    "nestia"
  ],
  "main": "./lib/index.mjs",
  "typings": "./lib/index.d.ts",
  "files": [
    "!src/examples",
    "LICENSE",
    "README.md",
    "dist",
    "lib",
    "package.json",
    "src"
  ],
  "scripts": {
    "build": "npm run build:static && npm run build:lib",
    "build:static": "rimraf dist && tsc -b && vite build",
    "build:lib": "rimraf lib && tsc --project tsconfig.lib.json --emitDeclarationOnly && rollup -c",
    "dev": "vite",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "prepare": "ts-patch install",
    "preview": "vite preview",
    "deploy": "node build/deploy.mjs",
    "prepack": "pnpm run build"
  },
  "dependencies": {
    "@agentica/core": "workspace:^",
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "^6.4.0",
    "@samchon/openapi": "catalog:typia",
    "html-to-image": "^1.11.13",
    "openai": "catalog:libs",
    "react-markdown": "^9.0.3",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-mermaid-plugin": "^1.0.2",
    "typia": "catalog:typia",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@ryoppippi/unplugin-typia": "catalog:typia",
    "@samchon/shopping-api": "^0.17.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "gh-pages": "^6.3.0",
    "globals": "^15.11.0",
    "js-yaml": "^4.1.0",
    "openai": "catalog:libs",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-mui-fileuploader": "^0.5.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.2",
    "ts-patch": "catalog:typescript",
    "typescript": "catalog:typescript",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9"
  }
}
