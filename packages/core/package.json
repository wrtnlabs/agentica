{
  "name": "@agentica/core",
  "version": "0.12.21",
  "main": "src/index.ts",
  "publishConfig": {
    "main": "lib/index.js",
    "module": "lib/index.mjs",
    "typings": "lib/index.d.ts",
    "access": "public"
  },
  "description": "Agentic AI Library specialized in LLM Function Calling",
  "scripts": {
    "prepare": "ts-patch install && pnpm build:prompt",
    "build": "rimraf lib && pnpm build:prompt && tsc && rollup -c",
    "build:prompt": "ts-node build/prompt.ts",
    "dev": "rimraf lib && tsc --watch",
    "eslint": "eslint ./**/*.ts",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint .",
    "format": "eslint --fix ."
  },
  "author": "Wrtn Technologies",
  "homepage": "https://wrtnlabs.io/agentica",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wrtnlabs/agentica"
  },
  "bugs": {
    "url": "https://github.com/wrtnlabs/agentica/issues"
  },
  "keywords": [
    "openai",
    "chatgpt",
    "anthropic",
    "claude",
    "ai",
    "chatbot",
    "nestia",
    "swagger",
    "openapi"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "lib",
    "prompts",
    "src"
  ],
  "devDependencies": {
    "@nestia/e2e": "^0.8.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.13.4",
    "@types/uuid": "^10.0.0",
    "openai": "^4.80.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.8",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "typedoc": "^0.27.7",
    "typescript": "~5.8.2"
  },
  "dependencies": {
    "@samchon/openapi": "^3.0.0",
    "typia": "^8.0.0",
    "uuid": "^11.0.4"
  },
  "peerDependencies": {
    "@samchon/openapi": ">=3.0.0 <4.0.0",
    "openai": ">=4.80.0"
  }
}
